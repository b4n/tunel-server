#include ".h"

void Derpm_Init (Derp *dm)
{

}
void Derpm_Clear (Derp *dm)
{

}
Derp* Derpm_New (void)
{
    Derp *dm = NULL;
    if (!(dm = SCE_malloc (sizeof *dm)))
        SCEE_LogSrc ();
    else
        Derpm_Init (dm);
    return dm;
}
void Derpm_Free (Derp *dm)
{
    if (dm) {
        Derpm_Clear (dm);
        SCE_free (dm);
    }
}
